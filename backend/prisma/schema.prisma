generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Task {
  idtask   Int      @id @default(autoincrement())
  titulo   String
  conteudo String
}

model User {
  iduser Int      @id @default(autoincrement())
  name   String
  email  String
  photo  String?
}
